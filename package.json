{
  "name": "oryx-macro-hax",
  "version": "2.0.0",
  "description": "Oryx Macro Post-Processor",
  "scripts": {
    "start": "dotenv cross-var ts-node src/main.ts",
    "get": "dotenv cross-var ts-node src/oryx-downloader.ts",
    "test": "jest",
    "lint": "tsc --noEmit",
    "format": "tsfmt -r",
    "copy": "cpy ./layout_src/**/keymap.c ./layout_src/**/config.h ./layout_src/**/rules.mk ../keyboards/moonlander/keymaps/hacked",
    "compile": "cd .. && qmk compile -kb moonlander -km hacked"
  },
  "author": "Phoscur & gittyeric",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "cpy-cli": "^4.2.0",
    "cross-var": "1.1.0",
    "dotenv-cli": "3.0.0",
    "parse-fraction": "^1.2.1",
    "pluralize": "^7.0.0",
    "prompts": "^2.4.2",
    "ts-node": "^7.0.1",
    "typescript-formatter": "^7.2.2",
    "unzipper": "^0.10.11"
  },
  "resolutions": {
    "json5": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/es6-promise": "^3.3.0",
    "@types/jquery": "^3.5.16",
    "@types/node": "^10.17.60",
    "@types/prompts": "^1.2.0",
    "@types/unzipper": "^0.10.5",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "typescript": "^5.0.4"
  }
}
